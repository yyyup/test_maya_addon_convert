{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "description": "Maya camera tools for Zoo Tools Pro",
    "displayName": "Zoo Camera Tools",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/zoo",
        "sourceFolder": "{self}/docs/source"
    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "ZOO_BASE_PATHS": [
            "{self}"
        ],
        "ZOO_ICON_PATHS": [
            "{self}/icons"
        ],
        "ZOO_MENU_LAYOUTS": [
