{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "commands": [
        "{self}/scripts/zoo_chatgpt_startup.py"
    ],
    "description": "Provides a Chat GPT UI embed in the host application using Zoo Tools Pro",
    "displayName": "Chat GPT",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/zoo",
        "sourceFolder": "{self}/docs/source"
    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "ZOO_BASE_PATHS": [
            "{self}"
        ],
        "ZOO_ICON_PATHS": [
            "{self}/icons"
        ],
        "ZOO_MENU_LAYOUTS": [
            "{self}/zoo/apps/integration/zoo_chatgpt_menu.layout"
        ],
        "ZOO_SHELF_LAYOUTS": [
            "{self}/zoo/apps/integration/zoo_chatgpt_shelf.layout"
        ],
        "ZOO_TOOLDEFINITION_MODULES": [
            "{self}/zoo/apps/integration/chatgpt_tooldefinitions.py"
        ]
    },
    "name": "zoo_chatgpt",
    "requirements": [
        "zoo_core",
        "zoo_pyside",
        "zoo_artist_palette"
    ],
    "version": "1.0.3"
}