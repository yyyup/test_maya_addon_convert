{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "description": "Lighting tools for Zoo Tools Pro",
    "displayName": "Zoo Light Suite",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/zoo",
        "sourceFolder": "{self}/docs/source"
    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "ZOO_BASE_PATHS": [
            "{self}"
        ],
        "ZOO_ICON_PATHS": [
            "{self}/icons"
        ],
        "ZOO_MENU_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_light_suite_menu.layout"
        ],
        "ZOO_SHELF_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_light_suite_shelf.layout"
        ],
        "ZOO_TOOLSET_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_light_suite_toolsets.layout"
        ],
        "ZOO_TOOLSET_UI_PATHS": [
            "{self}/zoo/apps/uitoolsets"
        ]
    },
    "name": "zoo_light_suite",
    "requirements": [
        "zoo_core",
        "zoo_preferences",
        "zoo_pyside",
        "zoo_maya",
        "zoo_toolsets",
        "zoo_artist_palette"
    ],
    "version": "1.3.25"
}