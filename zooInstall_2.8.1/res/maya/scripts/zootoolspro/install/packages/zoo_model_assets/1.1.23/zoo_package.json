    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "ZOO_BASE_PATHS": [
            "{self}"
        ],
        "ZOO_ICON_PATHS": [
            "{self}/icons"
        ],
        "ZOO_MENU_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_model_assets_menu.layout"
        ],
        "ZOO_SHELF_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_model_assets_shelf.layout"
