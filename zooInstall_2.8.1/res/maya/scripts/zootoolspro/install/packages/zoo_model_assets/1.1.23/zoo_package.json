{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "commands": [
        "{self}/scripts/zoo_model_assets_startup.py"
    ],
    "description": "Model Assets for Zoo Tools Pro",
    "displayName": "Zoo Model Assets",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/zoo",
