        "ZOO_TOOLDEFINITION_MODULES": [
            "{self}/zoo/apps/integration/maya/zoo_model_assets_tooldefinitions.py"
        ],
        "ZOO_TOOLSET_LAYOUTS": [
            "{self}/zoo/apps/integration/maya/zoo_model_assets_toolsets.layout"
        ],
        "ZOO_TOOLSET_UI_PATHS": [
            "{self}/zoo/apps/uitoolsets"
        ]
    },
    "name": "zoo_model_assets",
    "requirements": [
        "zoo_core",
        "zoo_preferences",
        "zoo_pyside",
        "zoo_maya",
