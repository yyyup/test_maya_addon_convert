{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "commands": [
        "{self}/scripts/zoo_toolsets_startup.py"
    ],
    "description": "Toolsets code repository for Zoo Tools Pro",
    "displayName": "Toolsets",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/zoo",
        "sourceFolder": "{self}/docs/source"
    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "ZOO_BASE_PATHS": [
            "{self}"
        ],
        "ZOO_ICON_PATHS": [
            "{self}/icons"
        ],
        "ZOO_TOOLDEFINITION_MODULES": [
            "{self}/zoo/apps/integration/all/zoo_toolsets_tooldefinitions.py"
        ],
        "ZOO_TOOLPALETTE_PLUGINTYPE_PATH": [
            "{self}/zoo/apps/integration/all/toolsetpalettetype.py"
        ],
        "ZOO_TOOLSET_UI_PATHS": [
            "{self}/zoo/apps/uitoolsets"
        ]
    },
    "name": "zoo_toolsets",
    "requirements": [
        "zoo_core",
        "zoo_pyside",
        "zoo_maya"
    ],
    "version": "1.3.28"
}